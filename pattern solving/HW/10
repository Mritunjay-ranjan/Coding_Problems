#include<iostream>
using namespace std;

int main(){
    int col;
    int n=5;
    for(int row=1;row<=n;row++){
        for(col=1;col<=row;col++){
            cout<<col;
        }
        col--;
        while(col>1){
            col--;
            cout<<col;
        }
        cout<<endl;

    }
    return 0;
}
