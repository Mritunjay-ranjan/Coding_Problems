#include<iostream>
using namespace std;

int main(){
    int n=5;
    for(int row=1;row<=n;row++){
        if(row==1||row==n){
            for(int col=1;col<=row;col++){
                cout<<col;
            }
        }
        else{
            for(int col=1;col<=row;col++){
                if(col==1||col==row){
                    cout<<col;
                }
                else{
                    cout<<" ";
                }
            }
        }
        cout<<endl;
    }
    return 0;
}
